<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Rapor_giris_ekrani">

    <Spinner
        android:id="@+id/spinner2"
        android:layout_width="303dp"
        android:layout_height="55dp"
        android:spinnerMode="dialog"
        tools:layout_editor_absoluteX="54dp"
        tools:layout_editor_absoluteY="180dp" />

    <EditText
        android:id="@+id/editText11"
        android:layout_width="325dp"
        android:layout_height="50dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text=""
        tools:layout_editor_absoluteX="43dp"
        tools:layout_editor_absoluteY="282dp" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Çalışılan Konu"
        android:textColor="#CB000000"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/editText11"
        app:layout_constraintStart_toStartOf="@+id/editText11" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="4dp"
        android:text="Ders seçin"
        android:textColor="#CB000000"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/spinner2"
        app:layout_constraintStart_toStartOf="@+id/spinner2" />

    <EditText
        android:id="@+id/editText12"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="number"
        tools:layout_editor_absoluteX="108dp"
        tools:layout_editor_absoluteY="383dp" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="4dp"
        android:text="Çözülen soru sayısı"
        android:textColor="#CB000000"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/editText12"
        app:layout_constraintStart_toStartOf="@+id/editText12" />

    <EditText
        android:id="@+id/editText13"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="numberSigned"
        tools:layout_editor_absoluteX="106dp"
        tools:layout_editor_absoluteY="471dp" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:text="Çalışılan dakika süresi"
        android:textColor="#CB000000"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/editText13"
        app:layout_constraintStart_toStartOf="@+id/editText13" />

    <Button
        android:id="@+id/button6"
        android:layout_width="97dp"
        android:layout_height="44dp"
        android:background="#ef7f1a"
        android:onClick="raporupload"
        android:text="Button"
        android:textColor="#FFFFFF"
        tools:layout_editor_absoluteX="260dp"
        tools:layout_editor_absoluteY="516dp" />

</androidx.constraintlayout.widget.ConstraintLayout>